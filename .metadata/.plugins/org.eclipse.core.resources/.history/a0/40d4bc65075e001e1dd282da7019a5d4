package com.krakedev;

public class Fecha 
{
	private int año;
	private int mes;
	private int dia;
	
	public int getAño()//get para recuperar
	{
		return año;
	}
	public int getMes()
	{
		return mes;
	}
	public int getDia()
	{
		return dia;
	}
	
	public void  setAño(int nuevoAño)//set para setear el valor
	{
		año = nuevoAño;
	}
	
	public void  setMes(int nuevoMes)
	{
		mes = nuevoMes;
	}
	
	public void  setDia(int nuevoDia)
	{
		dia = nuevoDia;
	}
	
	
}
